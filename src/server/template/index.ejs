    <% if(moduleCssArr.length){ %>
        <% moduleCssArr.forEach(function(item){ %>
            <link rel="stylesheet" type="text/css" href="<%= item %>">
        <% }) %>
    <% }%>
</head>
<body>
    <div id="root"><%- content %></div>
    <div id='pageData' hidden=true><%- totalData %></div>
    <div id='shade' style="display:none;"></div>
    <% if(baseVendorArr.length){ %>
        <% baseVendorArr.forEach(function(item){ %>
            <script src="<%= item %>"></script>
        <% }) %>
    <% }%>
    <% if(moduleJsArr.length){ %>
        <% moduleJsArr.forEach(function(item){ %>
            <script src="<%= item %>"></script>
        <% }) %>
    <% }%>
</body>
</html>